{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:posttwo/funnyjunk.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:posttwo/restcord.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:posttwo/socialite-discord"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ammadeuss/laravel-html-dom-parser": "^1.0",
        "arcanedev/log-viewer": "^4.3",
        "barryvdh/laravel-cors": "^0.9.2",
        "barryvdh/laravel-debugbar": "^2.3",
        "jenssegers/agent": "^2.5",
        "laravel/framework": "5.4.*",
        "laravel/passport": "^2.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "posttwo/funnyjunk": "dev-master",
        "posttwo/restcord": "dev-master",
        "posttwo/socialite-discord": "dev-master",
        "predis/predis": "^1.1",
        "spatie/laravel-permission": "^2.1",
        "team-reflex/discord-php": "^4.0",
        "watson/rememberable": "^2.0",
        "webpatser/laravel-uuid": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
